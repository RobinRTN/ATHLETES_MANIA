<div class="mt-5 mb-5">
  <h1>See what others think</h1>
    <% @reviews.each do |review|%>
      <div class="review-content">
        <h5><% review.rating.times do%>
          <i class="fa-solid fa-star"></i>
        <% end %></h5>
        <i><%= review.created_at.strftime('%Y-%m-%d') %></i>
        <p><%= review.content %></p>
      </div>
    <% end %>
</div>
